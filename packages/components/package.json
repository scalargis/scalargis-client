{
  "name": "@scalargis/components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "tsc": "tsc --project tsconfig.build.json",
    "build": "yarn tsc && yarn transpile && yarn copy",
    "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx' --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./dist/\""
  },
  "dependencies": {
    "@scalargis/jsonforms-primereact-renderers": "^3.1.0",
    "convert": "^5.4.1",
    "react-image-file-resizer": "^0.4.8",
    "fig-tree-evaluator": "^2.18.1",
    "object-property-assigner": "^1.3.1",
    "object-property-extractor": "^1.0.12"
  },
  "devDependencies": {
  } 
}